{
	"name": "n8n-nodes-wrangleai",
	"version": "1.1.7",
	"description": "WrangleAI- Optimize your ai usage",
	"license": "MIT",
	"homepage": "",
	"keywords": [
		"n8n-community-node-package",
		"n8n-community-node-wrangle"
	],
	"author": {
		"name": "WrangleAI Team",
		"email": "dave@wrangleai.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/WrangleAI/n8n-nodes-wrangleai.git"
	},
	"scripts": {
        "build":"n8n-node build && esbuild nodes/WrangleAi/WrangleAi.node.ts --bundle --platform=node --target=node18 --outfile=dist/nodes/WrangleAi/WrangleAi.node.js --external:n8n-workflow --allow-overwrite",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"test": "n8n-node lint"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/nodes/WrangleAi/WrangleAi.credentials.js"
		],
		"nodes": [
			"dist/nodes/WrangleAi/WrangleAi.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"esbuild": "^0.27.0",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
